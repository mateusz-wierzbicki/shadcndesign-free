{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "contact-section-1",
  "type": "registry:component",
  "title": "Contact Section 1",
  "description": "Contact form with validation and privacy policy checkbox",
  "registryDependencies": [
    "button",
    "input",
    "textarea",
    "checkbox",
    "field",
    "https://shadcndesign-free.vercel.app/r/styles.json",
    "https://shadcndesign-free.vercel.app/r/tagline.json"
  ],
  "files": [
    {
      "path": "components/pro-blocks/landing-page/contact-sections/contact-section-1.tsx",
      "content": "\"use client\";\n\nimport { Button } from \"@/components/ui/button\";\nimport { Input } from \"@/components/ui/input\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { Checkbox } from \"@/components/ui/checkbox\";\nimport { Field, FieldGroup, FieldLabel, FieldSet } from \"@/components/ui/field\";\nimport Link from \"next/link\";\nimport { Tagline } from \"@/components/pro-blocks/landing-page/tagline\";\n\nexport function ContactSection1() {\n  return (\n    <section\n      className=\"bg-background section-padding-y\"\n      aria-labelledby=\"contact-heading\"\n    >\n      <div className=\"mx-auto max-w-xl px-6\">\n        <div className=\"flex flex-col items-center gap-10 md:gap-12\">\n          {/* Section Title */}\n          <div className=\"section-title-gap-lg mx-auto flex max-w-xl flex-col items-center text-center\">\n            {/* Tagline */}\n            <Tagline>Contact Section</Tagline>\n            {/* Main Heading */}\n            <h1 id=\"contact-heading\" className=\"heading-lg\">\n              Get in touch\n            </h1>\n            {/* Description */}\n            <p className=\"text-muted-foreground\">\n              Write one or two welcoming sentences that encourage contact.\n              Include your response time commitment and highlight your\n              team&apos;s readiness to help.\n            </p>\n          </div>\n\n          {/* Contact Form */}\n          <form\n            className=\"flex w-full flex-col\"\n            onSubmit={(e) => e.preventDefault()}\n            aria-label=\"Contact form\"\n          >\n            <FieldSet>\n              <FieldGroup>\n                {/* Name Input */}\n                <Field>\n                  <FieldLabel htmlFor=\"name\">Name</FieldLabel>\n                  <Input\n                    id=\"name\"\n                    placeholder=\"Name\"\n                    required\n                    aria-required=\"true\"\n                  />\n                </Field>\n\n                {/* Email Input */}\n                <Field>\n                  <FieldLabel htmlFor=\"email\">Email</FieldLabel>\n                  <Input\n                    id=\"email\"\n                    placeholder=\"Email\"\n                    type=\"email\"\n                    required\n                    aria-required=\"true\"\n                  />\n                </Field>\n\n                {/* Message Textarea */}\n                <Field>\n                  <FieldLabel htmlFor=\"message\">Message</FieldLabel>\n                  <Textarea\n                    id=\"message\"\n                    placeholder=\"Type your message\"\n                    className=\"min-h-[106px]\"\n                    required\n                    aria-required=\"true\"\n                  />\n                </Field>\n\n                {/* Privacy Policy Checkbox */}\n                <Field orientation=\"horizontal\">\n                  <Checkbox id=\"privacy\" required aria-required=\"true\" />\n                  <FieldLabel\n                    htmlFor=\"privacy\"\n                    className=\"text-muted-foreground inline leading-none font-normal\"\n                  >\n                    By selecting this you agree to our{\" \"}\n                    <Link href=\"#\" className=\"text-foreground underline\">\n                      Privacy Policy\n                    </Link>\n                    .\n                  </FieldLabel>\n                </Field>\n\n                {/* Submit Button */}\n                <Field>\n                  <Button type=\"submit\" className=\"w-full\">\n                    Send message\n                  </Button>\n                </Field>\n              </FieldGroup>\n            </FieldSet>\n          </form>\n        </div>\n      </div>\n    </section>\n  );\n}\n",
      "type": "registry:component",
      "target": "components/pro-blocks/landing-page/contact-sections/contact-section-1.tsx"
    }
  ]
}